{
  "name": "pino-applicationinsights-transport",
  "version": "1.0.0",
  "description": "A pino v7+ transport for sending logs to Application Insights",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup src/index.ts",
    "format": "biome format src --write && biome format test --write",
    "lint": "biome lint src && biome lint test",
    "lint:check": "biome check --apply-unsafe src && check --apply-unsafe test",
    "lint:fix": "biome lint src && biome lint test --write",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "publish-package": "build && changeset version && changeset publish",
    "release": "changeset publish",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nathanlentz/pino-applicationinsights-transport.git"
  },
  "author": "Nate Lentz <n@natelentz.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanlentz/pino-applicationinsights-transport/issues"
  },
  "homepage": "",
  "keywords": [
    "pino",
    "applicationinsights",
    "transport",
    "typescript"
  ],
  "dependencies": {
    "applicationinsights": "^3.2.1",
    "p-retry": "^6.2.0",
    "pino-abstract-transport": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20.14.10",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3",
    "vitest": "^1.4.0"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "pnpm lint:fix"
    ]
  }
}
